{
  "name": "@cathedral/synth-lab-pro",
  "version": "1.0.0",
  "description": "ðŸŽ¹ Professional Synthesis Laboratory - Comprehensive audio libraries for creating real art with synth lab rooms",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./engines/*": {
      "types": "./dist/engines/*.d.ts",
      "import": "./dist/engines/*.esm.js",
      "require": "./dist/engines/*.js"
    },
    "./rooms/*": {
      "types": "./dist/rooms/*.d.ts",
      "import": "./dist/rooms/*.esm.js",
      "require": "./dist/rooms/*.js"
    },
    "./libraries/*": {
      "types": "./dist/libraries/*.d.ts",
      "import": "./dist/libraries/*.esm.js",
      "require": "./dist/libraries/*.js"
    }
  },
  "scripts": {
    "dev": "vite --host --port 3001",
    "build": "pnpm run build:clean && vite build && tsc --emitDeclarationOnly",
    "build:clean": "rimraf dist",
    "preview": "vite preview --port 3001",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "demo": "vite --open /demo/ --port 3001",
    "analyze": "vite-bundle-analyzer",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "audio:test": "node scripts/test-audio-libraries.js",
    "synth:rooms": "node scripts/generate-synth-rooms.js",
    "export:professional": "node scripts/export-professional-audio.js"
  },
  "dependencies": {
    "@cathedral/liber-arcanae-modular": "workspace:*",
    "@cathedral/p5-codex-engine": "workspace:*",
    
    "tone": "^14.7.77",
    "howler": "^2.2.4",
    "webmidi": "^3.1.8",
    "tonal": "^5.0.0",
    "chroma-js": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.6",
    "immer": "^10.0.3"
