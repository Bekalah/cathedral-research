{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Codex144 Node",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["node_id", "numerology", "geometry", "element"],
    "properties": {
      "node_id": { "type": "integer", "minimum": 1 },
      "numerology": { "type": "integer", "minimum": 1, "maximum": 9 },
      "geometry": { "type": "string" },
      "element": { "type": "string" },
      "planet": { "type": ["string", "null"] },
      "chakra": { "type": ["string", "null"] },
      "color_ray": { "type": ["string", "null"] },
      "sound": { "type": ["string", "null"] },
      "tarot_card": { "type": "array", "items": { "type": "string" } },
      "tara_overlay": { "type": ["string", "null"] },
      "crystal": { "type": ["string", "null"] },
      "symbol": { "type": ["string", "null"] },
      "teaching_function": { "type": ["string", "null"] },
      "related_gates": { "type": "array", "items": { "type": "integer" } },
      "daemon_guardian": { "type": ["string", "null"] },
      "notes": { "type": "string" }
    },
    "additionalProperties": false
  }
}
