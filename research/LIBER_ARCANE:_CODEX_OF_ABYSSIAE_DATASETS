// interfaces.d.ts
export type Ray = 1|2|3|4|5|6|7;

export interface Palette {
  id: string; label: string; ray?: Ray; biogeo?: string;
  colors: { primary: string; secondary?: string; halo?: string; ink?: string; gold?: string; };
}

export type LayerKind = "background"|"halo"|"glyph"|"sigil"|"text"|"audio-field";

export interface GeometryLayer {
  id: string; kind: LayerKind; props: Record<string, unknown>; provenance?: string;
}

export type NodeKind = "tarot"|"codex"|"room"|"lesson";

export interface Zodiac3 {
  sign: string; planet: string; degrees3: string; // "27-30 Cancer"
}

export interface CoreNode {
  id: string; kind: NodeKind; label: string; numerology: string;
  ray?: Ray; zodiac: Zodiac3; angel?: string; demon?: string;
  soyga?: string; crystal?: string; frequencyHz?: number;
  artifacts?: string[]; layersRef?: string[]; provenance?: string;
}

export interface CathedralInterchange {
  version: string;
  palettes: Palette[];
  geometryLayers: GeometryLayer[];
  nodes: CoreNode[];
}

BRIDGE HELPERS (ES MODULES, NO OVERWRITE)

// bridge-helpers.js  (ESM)
export async function loadJSON(url) {
  const r = await fetch(url, { cache: "no-store" });
  if (!r.ok) throw new Error("HTTP " + r.status + " " + url);
  return await r.json();
}

export function deepFreeze(o) {
  if (o && typeof o === "object") {
    Object.freeze(o);
    Object.getOwnPropertyNames(o).forEach(p => deepFreeze(o[p]));
  }
  return o;
}

export function namespacedId(repoKey, localId) {
  return `${repoKey}:${localId}`;
}

export function mergePalettes(...paletteSets) {
  const out = new Map();
  for (const set of paletteSets) {
    for (const p of set) {
      if (!out.has(p.id)) out.set(p.id, { ...p }); // first-wins, no overwrite
    }
  }
  return Array.from(out.values());
}

export function mergeLayers(...layerSets) {
  const out = new Map();
  for (const set of layerSets) {
    for (const L of set) {
      if (!out.has(L.id)) out.set(L.id, { ...L }); // first-wins
    }
  }
  return Array.from(out.values());
}

export function mergeNodes(...nodeSets) {
  const out = new Map();
  for (const set of nodeSets) {
    for (const n of set) {
      if (!out.has(n.id)) out.set(n.id, { ...n });
      // never overwrite existing keys; additional provenance can append
    }
  }
  return Array.from(out.values());
}

export function composeRendererState(registryRefs) {
  // registryRefs: [{repo:"liber-arcanae", data: CathedralInterchange}, ...]
  const palettes = mergePalettes(...registryRefs.map(r => r.data.palettes));
  const layers = mergeLayers(...registryRefs.map(r => r.data.geometryLayers));
  const nodes = mergeNodes(...registryRefs.map(r => r.data.nodes));
  return deepFreeze({ palettes, layers, nodes });
}

Usage:
// example: pulling from multiple repos
import { loadJSON, composeRendererState } from "./bridge-helpers.js";

const refs = await Promise.all([
  loadJSON("https://raw.githubusercontent.com/bekalah/liber-arcanae/main/assets/data/interchange.json").then(data => ({ repo:"liber-arcanae", data })),
  loadJSON("https://raw.githubusercontent.com/bekalah/codex-14499-core/main/assets/data/interchange.json").then(data => ({ repo:"codex-14499-core", data })),
  loadJSON("https://raw.githubusercontent.com/bekalah/stone-grimoire/main/assets/data/interchange.json").then(data => ({ repo:"stone-grimoire", data }))
]);

const RENDER = composeRendererState(refs);
// RENDER.palettes / .layers / .nodes are immutable snapshots

