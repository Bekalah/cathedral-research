2) /assets/css/codex.css
/* ND-safe defaults */
:root {
  --bg: #0b0e12;
  --ink: #e7e6ea;
  --accent: #bca66a;     /* alchemical gold */
  --octarine: #7f8cff;   /* octarine shimmer proxy */
  --lapis: #2b4c9f;
  --obsidian: #0a0b0c;
  --peacock: #0e7f72;
  --rose: #cc597c;

  --halo: rgba(127,140,255,0.22);
  --blur: 28px;
  --round: 14px;

  --safe-max-bright: 0.95;
  --safe-anim: 300ms;
}

/* Style packs (toggle by body class) */
body.theme-hermetic { --accent:#bca66a; --octarine:#7f8cff; --lapis:#2b4c9f; --peacock:#0e7f72; }
body.theme-thelemic { --accent:#caa33a; --octarine:#8a7bf2; --lapis:#3a49b7; --peacock:#2e6f64; }
body.theme-qabalah { --accent:#d4b96b; --octarine:#8b84ff; --lapis:#2a4bbf; --peacock:#19786d; }
body.theme-druidic { --accent:#a7c957; --octarine:#79a8ff; --lapis:#2a5b7f; --peacock:#0d8f6f; }
body.theme-vodou   { --accent:#d09d4d; --octarine:#a284ff; --lapis:#253c9b; --peacock:#0f6c5c; }
body.theme-alchemical { --accent:#c9a955; --octarine:#7b87ff; --lapis:#2747a6; --peacock:#0e7a68; }

/* ND-safe mode: reduce motion/contrast */
.nd-safe, .nd-safe * { animation-duration: var(--safe-anim); text-shadow: none !important; }

/* Layout */
html, body { margin:0; padding:0; background: var(--bg); color: var(--ink); font-family: ui-serif, Georgia, "Times New Roman", serif; }
header.skyline { display:flex; align-items:center; justify-content:space-between; padding:16px 22px; border-bottom:1px solid #1b1e24; }
header h1 { font-weight:700; letter-spacing:0.04em; }
header h1 span { color: var(--accent); }
.controls { display:flex; gap:10px; align-items:center; }
.controls select, .controls input, .controls button { background:#12151b; color:var(--ink); border:1px solid #232733; border-radius:8px; padding:8px 10px; }

.cathedral { display:grid; grid-template-columns: 320px 1fr; min-height: calc(100vh - 140px); }
.deck-list { border-right:1px solid #1b1e24; overflow:auto; padding:8px; }
.deck-item { padding:10px; border-radius:10px; margin:6px 4px; background:#0f131a; cursor:pointer; border:1px solid #20232e; }
.deck-item:hover { border-color: var(--accent); }
.deck-item .small { opacity:0.7; font-size:12px; }

.card-stage { display:flex; flex-direction:column; gap:18px; padding:18px; }
.abyssia-card { position:relative; width: 420px; height: 680px; border-radius: 16px; border:1px solid #2a2f3b; box-shadow: 0 0 30px rgba(0,0,0,.45); overflow:hidden; background:#0a0e13; }
.layer { position:absolute; inset:0; }
.layer.bg { background: radial-gradient(120% 100% at 50% 0%, #0a0f15 0%, #07090c 100%); }
.layer.pigment { mix-blend-mode: screen; opacity:0.4; background: conic-gradient(from 0deg, var(--lapis), var(--peacock), var(--octarine), var(--rose), var(--lapis)); filter: blur(20px); }
.layer.halo { pointer-events:none; background: radial-gradient(60% 40% at 50% 30%, var(--halo), transparent 70%); filter: blur(var(--blur)); }

.corners { position:absolute; inset:0; display:grid; grid-template-columns: 1fr 1fr; grid-template-rows:1fr 1fr; }
.corner { margin:10px; padding:6px 8px; border:1px solid #2b3240; border-radius:10px; background:#0d1219cc; font-size:12px; }
.tl{ align-self:start; justify-self:start; } .tr{ align-self:start; justify-self:end; }
.bl{ align-self:end; justify-self:start; } .br{ align-self:end; justify-self:end; }

.centerpiece { position:absolute; inset:80px 24px 80px 24px; display:grid; grid-template-rows: auto 1fr auto; gap:10px; }
.monad-fragments { position:relative; height:120px; filter: drop-shadow(0 5px 22px rgba(127,140,255,.25)); }
.monad-fragments .circle   { position:absolute; width:78px; height:78px; border:1px solid var(--accent); border-radius:50%; left:50%; top:10px; transform:translateX(-50%); }
.monad-fragments .crescent { position:absolute; width:40px; height:70px; border-left:1px solid var(--accent); border-radius:50%; left:calc(50% + 20px); top:18px; }
.monad-fragments .cross    { position:absolute; width:1px; height:90px; background:var(--accent); left:50%; top:10px; }
.monad-fragments .cross::after { content:""; position:absolute; width:60px; height:1px; background:var(--accent); left:-30px; top:45px; }
.monad-fragments .flame    { position:absolute; width:18px; height:26px; left:50%; top:92px; transform:translateX(-50%); border:1px solid var(--accent); border-radius: 12px 12px 50% 50% / 12px 12px 70% 70%; }

.sigil-grid { display:grid; grid-template-columns: 1fr 1fr 1fr; gap:10px; align-items:center; }
.sigil-grid .hebrew, .sigil-grid .abyssian, .sigil-grid .crystal {
  border:1px solid #2b3240; border-radius:12px; padding:10px; text-align:center; background:#0c1018a8; font-size:13px;
}
.figure { align-self:center; text-align:center; font-variant:small-caps; letter-spacing:0.1em; opacity:0.92; }

.banner { position:absolute; bottom:0; left:0; right:0; padding:10px 12px; background: linear-gradient(180deg, transparent, #0a0e13 40%, #090c10); border-top:1px solid #202330; }
.card-title { font-weight:700; letter-spacing:0.06em; color: var(--accent); }
.meta { margin-top:6px; display:flex; gap:6px; flex-wrap:wrap; }
.chip { border:1px solid #2b3240; padding:2px 6px; border-radius:999px; font-size:11px; background:#0d1219; }

.spreads { margin-top:12px; display:flex; align-items:center; gap:10px; }
