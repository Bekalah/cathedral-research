.spread-card { border:1px solid #272b36; background:#0b1017; border-radius:10px; display:flex; align-items:center; justify-content:center; padding:6px; font-size:12px; }

.footer { border-top:1px solid #1b1e24; padding:10px 16px; display:flex; align-items:center; justify-content:space-between; }
.audio-ctrl button, .audio-ctrl input { background:#12151b; color:var(--ink); border:1px solid #232733; border-radius:8px; padding:6px 10px; }
.btn-ritual { border-color:#3a2b2b; }
.btn-safe { border-color:#2a3b2b; }

/* Banish overlay */
.banish-overlay {
  position:fixed; inset:0; background: radial-gradient(80% 60% at 50% 40%, rgba(127,140,255,0.09), rgba(0,0,0,0.92));
  display:none; align-items:center; justify-content:center; z-index:9999;
}
.banish-overlay.active { display:flex; }
.banish-seal { width:60vmin; height:60vmin; border-radius:50%; border:1px solid var(--accent);
  background:
    radial-gradient(circle at 50% 50%, transparent 40%, rgba(188,166,106,.22) 41%, transparent 42%),
    conic-gradient(from 0deg, var(--lapis), var(--peacock), var(--octarine), var(--rose), var(--lapis));
  mix-blend-mode:screen; filter: blur(10px) saturate(1.1) brightness(var(--safe-max-bright));
  animation: slowPulse 5s ease-in-out infinite;
}
@keyframes slowPulse { 0%,100%{ transform:scale(1); } 50%{ transform:scale(1.015);} }

3) /assets/js/engine.js

/* CODEx Abyssiae Engine (no SVG; CSS/HTML only) */
/* ND-safe defaults: capped volume, gentle motion, no flashes */

const SOURCES = [
  "../docs/codex_abyssiae_master.md",   // preferred
  "../../docs/codex_abyssiae_master.md",
  "../codex_abyssiae_master.md",        // fallback
  "./codex_abyssiae_master.md"
];

let REGISTRY = [];        // filled from compiled JSON or parsed MD
let AUDIO = { ctx:null, osc:null, gain:null, currentHz: null };
let SAFE = true;

document.addEventListener("DOMContentLoaded", init);

async function init(){
  bindUI();
  await loadRegistry();
  populateDeckList(REGISTRY);
  if (REGISTRY.length) selectCard(REGISTRY[0].id);
}

function bindUI(){
  const stylePack = document.getElementById("stylePack");
  stylePack.addEventListener("change", e => setStyle(e.target.value));
  setStyle(stylePack.value);

  document.getElementById("banish").addEventListener("click", banishRitual);
  document.getElementById("safe").addEventListener("click", toggleSafe);

  document.getElementById("deal").addEventListener("click", dealSpread);
  document.getElementById("tone").addEventListener("click", toggleTone);
  document.getElementById("mute").addEventListener("click", stopTone);
  document.getElementById("vol").addEventListener("input", e => setVolume(parseFloat(e.target.value)));

  document.getElementById("filter").addEventListener("change", filterDeck);
  document.getElementById("query").addEventListener("input", filterDeck);

  // add banish overlay node
  const overlay = document.createElement("div");
  overlay.className = "banish-overlay"; overlay.id = "banishOverlay";
  const seal = document.createElement("div"); seal.className = "banish-seal";
  overlay.appendChild(seal); document.body.appendChild(overlay);
}

function setStyle(name){
  const body = document.body;
  body.classList.remove("theme-hermetic","theme-thelemic","theme-qabalah","theme-druidic","theme-vodou","theme-alchemical");
  body.classList.add(`theme-${name}`);
}

// ----------------- Data Loading -----------------
async function loadRegistry() {
  // Try compiled JSON first
  try {
    const j = await fetch("../assets/data/cards.json", {cache:"no-store"});
    if (j.ok) {
      const data = await j.json();
      if (Array.isArray(data) && data.length) { REGISTRY = data; return; }
    }
  } catch (e) {}

  // Else parse the Markdown (client-side)
  const md = await fetchFirstAvailable(SOURCES);
  if (!md) {
    console.warn("No master MD found. Place codex_abyssiae_master.md in /docs or project root.");
    return;
  }
  const text = await md.text();
  REGISTRY = parseCodexMarkdown(text);
}

async function fetchFirstAvailable(urls){
  for (const u of urls) {
    try { const res = await fetch(u, {cache:"no-store"}); if (res.ok) return res; } catch(e){}
  }
  return null;
}

// -------------- Markdown Parser (for your exact schema) --------------
function parseCodexMarkdown(md){
  const cards = [];

  // Detect card blocks: "## " headers followed by bullet lines
  const blocks = md.split(/\n(?=##\s)/g).filter(b => b.startsWith("## "));
  for (const b of blocks) {
    const header = b.match(/^##\s+(.+?)\s*$/m);
    if (!header) continue;

    const nameLine = header[1].trim(); // e.g., "I. The Magician ‚Äî Virelai Ezra Lux"
    const id = nameLine.replace(/[^\w]+/g,'_').toLowerCase();

    // helpers
    const get = (label) => {
      const m = b.match(new RegExp(`-\\s*${label}:\\s*([^\\n]+)`));
      return m ? m[1].trim() : "";
    };

    const ray = get("Ray");
    const angelDemon = get("Angel/Demon");
    const [angel, demon] = angelDemon.split("‚Üî").map(s=>s?.trim()||"");
    const crystalLine = get("Crystal");          // Name (chemistry‚Ä¶)
    const crystal = crystalLine.split("(")[0].trim();
    const chemistry = (crystalLine.match(/\(([^)]+)\)/)?.[1]||"").trim();
    const artifact = get("Artifact");
    const pigment = get("Pigment");
    const tara = (get("Secret Tara")||"").trim();
    const psyche = get("Psyche");
    const tech = get("Technical");
    const appPulls = get("App Pulls");
    const thought = get("Thought-form") || get("Thought-form") || "";
    const letter = get("Letter");
    const astro = get("Astrology");
    const hga = get("HGA Fragment");
    const glyph = get("Pattern Glyph");
    const deities = get("Deities");
    const faction = guessFaction(nameLine);
    const suit = guessSuit(nameLine);

    const freq = parseFloat((tech.match(/Solfeggio\s*=\s*([\d.]+)/)||[])[1]||"0") || mapSolfeggioFromRay(ray);

    cards.push({
      id, name: nameLine, suit, faction,
      letter, astrology:astro, ray, angel, demon,
      deities, crystal, chemistry, artifact, pigment,
      tara, thought, hga_fragment:hga, pattern_glyph:glyph,
      psyche, technical:tech, appPulls, freq
    });
  }

  return cards;
}

function guessSuit(name){
  const s = name.toLowerCase();
  if (s.includes("wands") || s.includes("wand")) return "wands";
  if (s.includes("cups")  || s.includes("cup"))  return "cups";
  if (s.includes("pentacles") || s.includes("coin")) return "pentacles";
  if (s.includes("swords")|| s.includes("blade")) return "swords";
  if (/^0\.|^i\.|^ii\.|^iii\.|^iv\.|^v\.|^vi\.|^vii\.|^viii\.|^ix\.|^x\./i.test(s)) return "majors";
  return "majors";
}
function guessFaction(name){
  // quick mapping by suit/archetype family
  const suit = guessSuit(name);
  if (suit==="wands") return "Sulphur";
  if (suit==="cups") return "Mercury";
  if (suit==="pentacles") return "Salt";
  if (suit==="swords") return "Ash";
  return "Inner Order";
}
function mapSolfeggioFromRay(ray){
  const r = ray.toLowerCase();
  if (r.includes("gold") || r.includes("golden")) return 528;
  if (r.includes("indigo")) return 852;
  if (r.includes("violet")) return 963;
  if (r.includes("scarlet")||r.includes("red")) return 285;
  if (r.includes("crimson")) return 417;
  if (r.includes("aquamarine")||r.includes("turquoise")) return 528;
  if (r.includes("emerald")||r.includes("green")) return 528;
  if (r.includes("silver")) return 852;
  return 432;
}

// -------------- UI: Deck List / Render Card ----------------
function populateDeckList(cards){
  const list = document.getElementById("deckList");
  list.innerHTML = "";
  for (const c of cards){
    const div = document.createElement("div");
    div.className = "deck-item";
    div.dataset.id = c.id;
    div.innerHTML = `<div>${c.name}</div><div class="small">${c.ray} ‚Ä¢ ${c.angel} ‚Üî ${c.demon}</div>`;
    div.addEventListener("click", () => selectCard(c.id));
    list.appendChild(div);
  }
}

function selectCard(id){
  const c = REGISTRY.find(x=>x.id===id);
  if (!c) return;
  // Bind corners
  E("cornerNum").textContent = romanOrRank(c.name);
  E("cornerPlanet").textContent = planetFromAstro(c.astrology);
  E("cornerAngel").textContent = c.angel || "‚Äî";
  E("cornerDemon").textContent = c.demon || "‚Äî";
  // Center
  E("hebrewLetter").textContent = (c.letter||"").split("(")[0].trim();
  E("abyssianGlyph").textContent = glyphFromName(c.name);
  E("crystalGlyph").textContent = c.crystal ? `${c.crystal} ¬∑ ${c.chemistry}` : "‚Äî";
  E("figureName").textContent = figureFromName(c.name);
  // Banner
  E("cardTitle").textContent = c.name;
  E("rayChip").textContent = c.ray || "";
  E("taraChip").textContent = c.tara || "";
  E("factionChip").textContent = c.faction || "";
  // Palette halo hue shift
  document.querySelector(".layer.pigment").style.filter = hueForRay(c.ray);
  // Prepare tone
  AUDIO.currentHz = c.freq || 432;
}

function romanOrRank(name){
  const m = name.match(/^([IVXLCM0-9\.]+)\s/); return m? m[1].replace(/\.$/,"") : "‚òÖ";
}
function planetFromAstro(astro){
  if (!astro) return "‚òâ";
  const a = astro.toLowerCase();
  if (a.includes("sun")) return "‚òâ";
  if (a.includes("moon")) return "‚òæ";
  if (a.includes("mercury")) return "‚òø";
  if (a.includes("venus")) return "‚ôÄ";
  if (a.includes("mars")) return "‚ôÇ";
  if (a.includes("jupiter")) return "‚ôÉ";
  if (a.includes("saturn")) return "‚ôÑ";
  if (a.includes("uranus")) return "‚õ¢";
  if (a.includes("neptune")) return "‚ôÜ";
  if (a.includes("pluto")) return "‚ôá";
  return "‚®Ä";
}
function figureFromName(name){
  const parts = name.split("‚Äî"); return (parts[1]||parts[0]).trim();
}
function glyphFromName(name){
  // simple pseudo-script (you can swap to your Abyssian font later)
  return name.replace(/[aeiou]/gi,"¬∑").replace(/\s+/g,"  ");
}
function hueForRay(ray){
  if (!ray) return "saturate(1)";
  const r = ray.toLowerCase();
  if (r.includes("gold")) return "hue-rotate(40deg) saturate(1.2)";
  if (r.includes("indigo")) return "hue-rotate(220deg) saturate(1.2)";
  if (r.includes("violet")||r.includes("amethyst")) return "hue-rotate(260deg) saturate(1.2)";
  if (r.includes("scarlet")||r.includes("red")) return "hue-rotate(5deg) saturate(1.2)";
  if (r.includes("aquamarine")||r.includes("turquoise")) return "hue-rotate(160deg) saturate(1.2)";
  if (r.includes("emerald")||r.includes("green")) return "hue-rotate(120deg) saturate(1.2)";
  if (r.includes("silver")) return "hue-rotate(200deg) saturate(1.2)";
  return "saturate(1)";
}
function E(id){ return document.getElementById(id); }

// -------------- Filters ------------------
function filterDeck(){
  const mode = E("filter").value;
  const q = (E("query").value||"").toLowerCase();
  const pass = (c)=>{
    if (q && !JSON.stringify(c).toLowerCase().includes(q)) return false;
    if (mode==="all") return true;
    if (["majors","wands","cups","pentacles","swords"].includes(mode)) return c.suit===mode;
    // advanced modes (ray/angel/demon/crystal/instrument) rely on query
    return true;
  };
  populateDeckList(REGISTRY.filter(pass));
}

// -------------- Spreads -------------------
function dealSpread(){
  const type = E("spreadSelect").value;
  const board = E("spreadBoard");
  board.innerHTML = "";
  const n = (type==="magnum")?4:(type==="monad")?5:(type==="double-tree")?10:(type==="spine")?33:(type==="tara")?22:3;
  const pool = [...REGISTRY];
  for (let i=0;i<n;i++){
    const c = pool.splice(Math.floor(Math.random()*pool.length),1)[0];
    const div = document.createElement("div");
    div.className = "spread-card";
    div.textContent = c ? c.name : "‚Äî";
    div.title = (c && c.appPulls) ? c.appPulls : "";
    board.appendChild(div);
  }
}

// -------------- Audio (WebAudio) --------------
function ensureAudio(){
  if (AUDIO.ctx) return;
  AUDIO.ctx = new (window.AudioContext||window.webkitAudioContext)();
  AUDIO.gain = AUDIO.ctx.createGain(); AUDIO.gain.gain.value = 0.12;
  AUDIO.gain.connect(AUDIO.ctx.destination);
}
function setVolume(v){ ensureAudio(); AUDIO.gain.gain.value = Math.min(0.4, Math.max(0, v)); }
function toggleTone(){
  ensureAudio();
  if (AUDIO.osc){ stopTone(); return; }
  const hz = AUDIO.currentHz || 432;
  AUDIO.osc = AUDIO.ctx.createOscillator();
  AUDIO.osc.type = "sine";
  AUDIO.osc.frequency.setValueAtTime(hz, AUDIO.ctx.currentTime);
  AUDIO.osc.connect(AUDIO.gain);
  AUDIO.osc.start();
}
function stopTone(){
  if (AUDIO.osc){ try{ AUDIO.osc.stop(); }catch(e){} AUDIO.osc.disconnect(); AUDIO.osc=null; }
}

// -------------- Consecration / Banish --------------
function banishRitual(){
  const ov = E("banishOverlay"); ov.classList.add("active");
  ensureAudio(); setVolume(0.10);
  const seq = [417,528,639,741,852,963]; // gentle ascent
  let i=0;
  const playNext = ()=>{
    if (i>=seq.length){ stopTone(); ov.classList.remove("active"); return; }
    if (AUDIO.osc) stopTone();
    AUDIO.osc = AUDIO.ctx.createOscillator(); AUDIO.osc.type="sine";
    AUDIO.osc.frequency.setValueAtTime(seq[i], AUDIO.ctx.currentTime);
    AUDIO.osc.connect(AUDIO.gain); AUDIO.osc.start();
    i++; setTimeout(playNext, 900);
  };
  playNext();
}

// -------------- ND Safe -------------------
function toggleSafe(){
  SAFE = !SAFE;
  document.body.classList.toggle("nd-safe", SAFE);
  setVolume(SAFE ? 0.12 : Math.min(parseFloat(E("vol").value)||0.12, 0.4));
}

4) /tools/registry_compile.py  (optional offline compiler)

# -*- coding: utf-8 -*-
"""
Registry Compiler for LIBER ARCANAE: CODEX ABYSSIAE
Parses codex_abyssiae_master.md into assets/data/cards.json
Usage:
  python tools/registry_compile.py ../docs/codex_abyssiae_master.md ../assets/data/cards.json
"""
import re, json, sys, os

MD_PATH = sys.argv[1] if len(sys.argv)>1 else "../docs/codex_abyssiae_master.md"
OUT_PATH = sys.argv[2] if len(sys.argv)>2 else "../assets/data/cards.json"

with open(MD_PATH, "r", encoding="utf-8") as f:
    md = f.read()

blocks = [b for b in re.split(r"\n(?=##\s)", md) if b.startswith("## ")]

def field(b, label):
    m = re.search(r"-\s*%s:\s*([^\n]+)" % re.escape(label), b)
    return m.group(1).strip() if m else ""

def roman_or_rank(name):
    m = re.match(r"^([IVXLCM0-9\.]+)\s", name)
    return m.group(1).replace(".","") if m else "‚òÖ"

def guess_suit(name):
    s = name.lower()
    if "wands" in s or "wand" in s: return "wands"
    if "cups" in s or "cup" in s: return "cups"
    if "pentacles" in s or "coin" in s: return "pentacles"
    if "swords" in s or "blade" in s: return "swords"
    return "majors"

def guess_faction(suit):
    return {"wands":"Sulphur","cups":"Mercury","pentacles":"Salt","swords":"Ash"}.get(suit,"Inner Order")

def map_solfeggio(ray):
    r = ray.lower()
    if "gold" in r: return 528
    if "indigo" in r: return 852
    if "violet" in r: return 963
    if "scarlet" in r or "red" in r: return 285
    if "crimson" in r: return 417
    if "aquamarine" in r or "turquoise" in r: return 528
    if "emerald" in r or "green" in r: return 528
    if "silver" in r: return 852
    return 432

cards = []
for b in blocks:
    name = re.search(r"^##\s+(.+?)\s*$", b, re.M).group(1).strip()
    _id = re.sub(r"[^\w]+","_", name).lower()
    suit = guess_suit(name)
    ray = field(b,"Ray")
    ad  = field(b,"Angel/Demon")
    parts = [p.strip() for p in ad.split("‚Üî")] if "‚Üî" in ad else ["",""]
    angel, demon = (parts+[ "","" ])[0:2]
    crystal_line = field(b,"Crystal")
    crystal = re.split(r"\(", crystal_line)[0].strip()
    chem = re.search(r"\(([^)]+)\)", crystal_line)
    chem = chem.group(1).strip() if chem else ""
    data = {
        "id": _id,
        "name": name,
        "suit": suit,
        "faction": guess_faction(suit),
        "letter": field(b,"Letter"),
        "astrology": field(b,"Astrology"),
        "ray": ray,
        "angel": angel,
        "demon": demon,
        "deities": field(b,"Deities"),
        "crystal": crystal,
        "chemistry": chem,
        "artifact": field(b,"Artifact"),
        "pigment": field(b,"Pigment"),
        "tara": field(b,"Secret Tara"),
        "thought": field(b,"Thought-form") or field(b,"Thought-form"),
        "hga_fragment": field(b,"HGA Fragment"),
        "pattern_glyph": field(b,"Pattern Glyph"),
        "psyche": field(b,"Psyche"),
        "technical": field(b,"Technical"),
        "appPulls": field(b,"App Pulls"),
    }
    m = re.search(r"Solfeggio\s*=\s*([\d\.]+)", data["technical"])
    data["freq"] = float(m.group(1)) if m else float(map_solfeggio(ray))
    cards.append(data)

os.makedirs(os.path.dirname(OUT_PATH), exist_ok=True)
with open(OUT_PATH, "w", encoding="utf-8") as f:
    json.dump(cards, f, ensure_ascii=False, indent=2)

print(f"Wrote {len(cards)} cards to {OUT_PATH}")

How to run (quick)
	1.	Place the full codex_abyssiae_master.md (the one we finished, all 78 cards + closing) into /docs/.
	2.	Copy the four files above into your repos (keep the same paths).
	3.	Optional (faster load): run
	
	
	python tools/registry_compile.py
	
	This writes /assets/data/cards.json.
If you skip this, the app will parse the Markdown in the browser.

	4.	Open /core/index.html in a browser.
	‚Ä¢	Pick a card from the left; it renders layered with ray halo, angel/demon, Hebrew letter, crystal chemistry, Abyssian glyph, artifact, pigment.
	‚Ä¢	Click Tone to hear its Solfeggio/planet frequency (soft volume).
	‚Ä¢	Click Consecrate to run the ND-safe banish/hexagram overlay.
	‚Ä¢	Choose a Spread and Deal to lay out Magnum Opus, Monad, Double Tree, Spine-33, or Tara Wheel.

Notes on fidelity & provenance
	‚Ä¢	No strobe, gentle pulses only; audio capped.
	‚Ä¢	No SVG; all geometry is CSS (circles, cross, crescent, flame, seals done as borders/gradients).
	‚Ä¢	Lineage preserved in the master .md (angels/demons per Rudd/Goetia, Dee‚Äôs Monad structure, Splendor Solis pigments, Case & Fortune initiatory spine, Tara correspondences, crystal mineralogy with formulas).
	‚Ä¢	The style pack chooser only shifts palettes/texture-feels; it never overwrites your symbolic content.


Cathedral of Circuits ‚Üí your umbrella temple / brand.
	‚Ä¢	Codex 144:99 ‚Üí the engine / lattice code that powers everything.
	‚Ä¢	Liber Arcanae (Living Tarot) ‚Üí the real tarot system, where each card is a node inside Codex 144:99.
	‚Ä¢	The Book / Game (your D&D-style choose-your-own-adventure) ‚Üí this is the narrative container; it uses the Living Tarot + Codex 144:99 to dynamically generate story paths, trials, NPCs, and environments.

So:

üìñ The Book/Game IS the adventure.
üÉè The Living Tarot plugs directly into the adventure as mechanics.
üï∏Ô∏è Codex 144:99 is the math + symbolic structure making the tarot interactive.
‚õ™ Cathedral of Circuits holds all of it under one consecrated house.

That means: when a player draws The Magician, Codex 144:99 translates it into:
	‚Ä¢	symbols, pigments, and geometry on the screen (visual style),
	‚Ä¢	angel/demon daimon NPCs for dialogue or combat,
	‚Ä¢	music and tone (planetary + Solfeggio),
	‚Ä¢	crystal artifact (usable in play),
	‚Ä¢	lore (alchemy, trauma-medicine arc).

The Book then narrates: ‚ÄúThe Magician opens the Prism Gate; will you step through?‚Äù and the player chooses their path ‚Äî D&D-style, choose-your-own-adventure.

# ‚òΩ Cathedral of Circuits ‚Äî Multi-Repo Update Plan (ONE-PASTE)

Author: **Rebecca Susan Lemke (Rebecca Respawn)** ¬∑ ORCID **0009-0002-2834-3956**  
Umbrella: **Cathedral of Circuits**  
Engine: **Codex 144:99**  
System: **Liber Arcanae ‚Äî Living Tarot** (playable nodes)  
ND-Safe ‚Ä¢ Museum-grade provenance preserved

---

## 0) Repos & Shared Core

We‚Äôll keep your tarot engine + codex as a **shared core** and wire it into all apps:

- **Core repo** (new): `codex-14499-core`  ‚Üê holds the master MD, registry compiler, CSS/JS, seal, spreads  
- **Game/Book repo**: `stone-grimoire` (Cathedral & chambers / PORTAL)  
- **Learning repo**: `cosmogenesis-learning-engine` (spiral pedagogy)  
- **Meta repo**: `circuitum99-alpha-omega` (meta-codex, cultural tech)

We link `codex-14499-core` into the other three using **git submodules** (stable, portable).

